{
    "name":        "arcanedev/laravel-settings",
    "description": "This package allows you to persists configs/settings for Laravel projects.",
    "homepage":    "https://github.com/ARCANEDEV/LaravelSettings",
    "keywords":    [
        "arcanedev", "laravel", "config", "settings", "json", "array", "database", "db"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=5.6",
        "arcanedev/support": "~3.0"
    },
    "require-dev": {
        "phpunit/phpcov":   "~3.0",
        "phpunit/phpunit":  "~5.0",
        "predis/predis":    "^1.1",
        "illuminate/redis": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelSettings\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelSettings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\" \"orchestra/database=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "illuminate/redis": "This package allows you to persist the settings into a redis server."
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
